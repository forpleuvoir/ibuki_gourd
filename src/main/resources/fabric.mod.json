{
  "schemaVersion": 1,
  "id": "ibuki_gourd",
  "version": "${version}",
  "name": "Ibuki Gourd",
  "description": "可能是个前置库",
  "authors": [
    "forpleuvoir"
  ],
  "contact": {
    "email": "695801070@qq.com",
    "website": "还没有",
    "repo": "还没有"
  },
  "license": "LGPLv3",
  "icon": "assets/ibuki_gourd/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "forpleuvoir.ibuki_gourd.mod.IbukiGourdMod"
      }
    ],
    "modmenu": [
      {
        "adapter": "kotlin",
        "value": "forpleuvoir.ibuki_gourd.mod.compat.modmenu.ModMenuImpl"
      }
    ],
    "ibScreen": [
      {
        "adapter": "kotlin",
        "value": "forpleuvoir.ibuki_gourd.mod.compat.ibuki_gourd.ScreenImpl"
      }
    ],
    "ibConfig": [
      {
        "adapter": "kotlin",
        "value": "forpleuvoir.ibuki_gourd.mod.compat.ibuki_gourd.ConfigImpl"
      }
    ]
  },
  "custom": {
    "modmenu": {
      "badges": [
        "library"
      ]
    }
  },
  "mixins": [
    "ibuki_gourd.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": "*",
    "minecraft": "1.19.x",
    "fabric-language-kotlin": ">=1.8.0+kotlin.1.7.0"
  },
  "suggests": {
    "another-mod": "modmenu"
  }
}
